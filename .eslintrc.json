{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "standard-with-typescript",
    "plugin:react/recommended",
    "plugin:prettier/recommended",
    "plugin:import/recommended",
    "plugin:import/typescript"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": [
    "react",
    "prettier"
  ],
  "rules": {
    "no-console": "warn",
    "import/no-unused-modules": [
      "warn",
      {
        "unusedExports": true
      }
    ]
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}